set nocompatible
filetype on
filetype plugin on
filetype indent on
syntax on
set number relativenumber
set tabstop=4
set shiftwidth=4
set autoindent
set mouse=a
set scrolloff=10
set incsearch
set showcmd
set showmatch
set history=1000
set wildmenu
set wildmode=list:longest
set wildignore=*.docx,*.jpg,*.png,*.gif,*.pdf,*.pyc,*.exe,*.flv,*.img,*.xlsx
colorscheme pablo