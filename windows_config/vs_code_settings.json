{
    // Vim Settings
    // "editor.lineNumbers": "relative",
    // Personal Settings
    "editor.fontFamily": "CaskaydiaMono Nerd Font Mono",
    "editor.inlineSuggest.fontFamily": "CaskaydiaMono Nerd Font Mono",
    "editor.codeLensFontFamily": "CaskaydiaMono Nerd Font Mono",
    "debug.console.fontFamily": "CaskaydiaMono Nerd Font Mono",
    "scm.inputFontFamily": "CaskaydiaMono Nerd Font Mono",
    "terminal.integrated.fontFamily": "CaskaydiaMono Nerd Font Mono",
    "notebook.output.fontFamily": "CaskaydiaMono Nerd Font Mono",
    "chat.editor.fontFamily": "CaskaydiaMono Nerd Font Mono",
    "[mojo]": {
        "editor.formatOnSave": true
    },
    "python.analysis.typeCheckingMode": "basic",
    "editor.codeActionsOnSave": {
        "source.organizeImports": "always"
    },
    "editor.hover.enabled": true,
    "scm.defaultViewMode": "tree",
    "[python]": {
        "editor.formatOnType": true,
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSaveMode": "file",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        },
        "diffEditor.ignoreTrimWhitespace": true
    },
    "black-formatter.showNotifications": "onWarning",
    "jupyter.enableKernelCompletions": true,
    "jupyter.executionAnalysis.enabled": true,
    "jupyter.interactiveWindow.textEditor.cellFolding": true,
    "jupyter.stopOnFirstLineWhileDebugging": true,
    "jupyter.disableJupyterAutoStart": false,
    "notebook.lineNumbers": "on",
    "notebook.cellFocusIndicator": "gutter",
    "notebook.formatOnCellExecution": true,
    "notebook.formatOnSave.enabled": false,
    "notebook.showCellStatusBar": "visible",
    "notebook.showFoldingControls": "always",
    "notebook.undoRedoPerCell": true,
    "notebook.codeActionsOnSave": {
        "source.organizeImports": "explicit"
    },
    "[yaml]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced",
        "diffEditor.ignoreTrimWhitespace": false
    },
    "[json]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.suggest.insertMode": "replace",
        "editor.formatOnSave": true,
    },
    "[markdown]": {
        "files.autoSave": "afterDelay",
        "files.autoSaveDelay": 1000,
        "editor.unicodeHighlight.ambiguousCharacters": false,
        "editor.unicodeHighlight.invisibleCharacters": false,
        "diffEditor.ignoreTrimWhitespace": false,
        "editor.wordWrap": "on",
        "editor.quickSuggestions": {
            "comments": "off",
            "strings": "off",
            "other": "off"
        }
    },
    "markdown.editor.pasteUrlAsFormattedLink.enabled": "smartWithSelection",
    "editor.rulers": [
        90,
        120
    ],
    // Default Settings
    "git.autofetch": true,
    "git.confirmSync": false,
    "workbench.iconTheme": "material-icon-theme",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.languageServer": "Pylance",
    "python.terminal.activateEnvironment": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "*.bmp": "default"
    },
    "jupyter.askForKernelRestart": false,
    "editor.wordWrap": "wordWrapColumn",
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": true,
        "markdown": true,
        "scminput": false,
        "yaml": true,
        "mojo": true
    },
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "editor.multiCursorModifier": "ctrlCmd",
    "workbench.colorCustomizations": {
        "terminal.foreground": "#00FD61",
        "terminal.background": "#383737",
        "editorRuler.foreground": "#19a8ad"
    },
    "material-icon-theme.folders.color": "#90a4ae",
    "material-icon-theme.opacity": 1,
    "material-icon-theme.activeIconPack": "angular_ngrx",
    "editor.largeFileOptimizations": false,
    "editor.minimap.maxColumn": 90,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.size": "fill",
    "zenMode.centerLayout": false,
    "zenMode.hideLineNumbers": false,
    "git.ignoreLegacyWarning": true,
    "workbench.startupEditor": "welcomePageInEmptyWorkbench",
    "notebook.output.textLineLimit": 70,
    "editor.accessibilitySupport": "off",
    "jupyter.widgetScriptSources": [
        "jsdelivr.com",
        "unpkg.com"
    ],
    "git.openRepositoryInParentFolders": "never",
    "editor.wordWrapColumn": 90,
    "diffEditor.experimental.showMoves": true,
    "workbench.activityBar.location": "top",
    "workbench.tree.enableStickyScroll": false,
    "notebook.stickyScroll.enabled": true,
    "jupyter.debugJustMyCode": false,
    "debugpy.debugJustMyCode": false,
    "diffEditor.maxComputationTime": 0,
    "diffEditor.ignoreTrimWhitespace": false,
    "accessibility.underlineLinks": true,
    "editor.minimap.enabled": false,
    "diffEditor.hideUnchangedRegions.enabled": true,
    "github.copilot.nextEditSuggestions.enabled": true,
    "update.mode": "none",
    "explorer.confirmDelete": false,
    "jupyter.themeMatplotlibPlots": true,
    "window.autoDetectColorScheme": true,
    "markdown-preview-github-styles.darkTheme": "dark_dimmed",
    "workbench.colorTheme": "Atom One Dark",
    "workbench.preferredDarkColorTheme": "Atom One Dark"
}